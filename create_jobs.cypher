UNWIND range(0, 899) as job_idx
WITH businesses, job_titles, job_idx, businesses[job_idx % size(businesses)] as b, job_titles[job_idx % size(job_titles)] as base_title
CREATE (job:Job {
  id: randomUuid(),
  title: base_title + " (" + toString(job_idx + 1) + ")",
  description: "We are hiring a " + base_title + " for " + b.name + ". You will be responsible for various tasks. This is an excellent opportunity to grow your career with a reputable company in Catanduanes.",
