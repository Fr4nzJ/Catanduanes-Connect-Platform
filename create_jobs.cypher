  category: b.category,
  employment_type: "Full-time",
  salary_min: 15000,
  salary_max: 50000,
  location: b.address,
  skills_required: ["communication", "teamwork", "problem-solving"],
  benefits: ["Health Insurance", "Paid Leave", "Training"],
  deadline: date() + duration({days: 30}),
  posted_date: date(),
  created_at: toString(datetime()),
  updated_at: toString(datetime()),
  is_active: true
})
CREATE (job)-[:POSTED_BY]->(b)
WITH b, count(*) as job_count
RETURN b.name as business, job_count
